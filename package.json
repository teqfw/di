{
  "name": "@teqfw/di",
  "version": "0.4.1",
  "description": "Dependency Injection container based on ES6 modules (works for browsers & nodejs).",
  "type": "module",
  "main": "src/Shared/Container.mjs",
  "scripts": {
    "example": "node --experimental-modules ./example/s001/s001.mjs",
    "test": "./node_modules/mocha/bin/mocha --recursive './test/**/*.test.mjs'",
    "jsdoc": "./node_modules/jsdoc/jsdoc.js -d ./jsdoc/ -a all -c ./.jsdoc"
  },
  "license": "GPL-3.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/teqfw/di.git"
  },
  "devDependencies": {
    "babel-eslint": "*",
    "eslint": "^7.24.0",
    "esm": "*",
    "jsdoc": "*",
    "mocha": "^8.3.2"
  },
  "esm": {
    "cjs": true
  },
  "teqfw": {
    "autoload": {
      "ns": "TeqFw_Di",
      "path": "./src"
    }
  }
}
