{
  "name": "@teqfw/di",
  "version": "1.1.2",
  "description": "Dependency Injection container for ES6 modules that works in both browser and Node.js apps.",
  "keywords": [
    "dependency injection",
    "di",
    "dynamic import",
    "es6 modules",
    "teqfw",
    "tequila framework"
  ],
  "homepage": "https://github.com/teqfw/di#readme",
  "bugs": {
    "url": "https://github.com/teqfw/di/issues"
  },
  "license": "Apache-2.0",
  "author": {
    "name": "Alex Gusev",
    "email": "alex@flancer64.com",
    "url": "https://github.com/flancer64"
  },
  "files": [
    "dist/",
    "src/",
    "CHANGELOG.md",
    "LICENSE",
    "README.md",
    "teqfw.json",
    "types.d.ts"
  ],
  "main": "dist/umd.js",
  "module": "dist/esm.js",
  "types": "types.d.ts",
  "exports": {
    ".": {
      "import": "./dist/esm.js",
      "require": "./dist/umd.js"
    },
    "./pre/replace": {
      "import": "./src/Pre/Replace.js"
    }
  },
  "type": "module",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/teqfw/di.git"
  },
  "scripts": {
    "rollup": "rollup -c",
    "eslint": "eslint './src/**/*.js'",
    "test": "node --test"
  },
  "devDependencies": {
    "@eslint/js": "^9.33.0",
    "@rollup/plugin-node-resolve": "^16.0.1",
    "@rollup/plugin-terser": "^0.4.4",
    "eslint": "^9.33.0",
    "rollup": "^4.47.1"
  },
  "engines": {
    "node": ">=20"
  },
  "funding": {
    "type": "individual",
    "url": "https://github.com/sponsors/teqfw"
  }
}
